# Test Environment Configuration for Vibe Coder
# This file contains test-specific environment variables
# It should never contain real API keys or sensitive data

# Test Configuration
VIBE_CODER_ENV=test
VIBE_CODER_LOG_LEVEL=DEBUG
VIBE_CODER_CONFIG_DIR=/tmp/vibe_coder_test
VIBE_CODER_CACHE_DIR=/tmp/vibe_coder_test/cache

# Mock OpenAI Configuration (for testing)
VIBE_CODER_OPENAI_API_KEY=sk-test-key-12345
VIBE_CODER_OPENAI_ENDPOINT=https://api.openai.com/v1
VIBE_CODER_OPENAI_MODEL=gpt-3.5-turbo
VIBE_CODER_OPENAI_TEMPERATURE=0.7
VIBE_CODER_OPENAI_MAX_TOKENS=1000

# Mock Anthropic Configuration (for testing)
VIBE_CODER_ANTHROPIC_API_KEY=sk-ant-test-key-67890
VIBE_CODER_ANTHROPIC_ENDPOINT=https://api.anthropic.com
VIBE_CODER_ANTHROPIC_MODEL=claude-3-haiku-20240307
VIBE_CODER_ANTHROPIC_TEMPERATURE=0.5
VIBE_CODER_ANTHROPIC_MAX_TOKENS=2000

# Mock Ollama Configuration (for testing)
VIBE_CODER_OLLAMA_API_KEY=test-key
VIBE_CODER_OLLAMA_ENDPOINT=http://localhost:11434
VIBE_CODER_OLLAMA_MODEL=llama2
VIBE_CODER_OLLAMA_TEMPERATURE=0.8
VIBE_CODER_OLLAMA_MAX_TOKENS=1500

# Mock Generic Provider Configuration (for testing)
VIBE_CODER_GENERIC_API_KEY=generic-test-key
VIBE_CODER_GENERIC_ENDPOINT=https://api.generic-endpoint.com/v1
VIBE_CODER_GENERIC_MODEL=generic-model
VIBE_CODER_GENERIC_TEMPERATURE=0.6
VIBE_CODER_GENERIC_MAX_TOKENS=1200

# Default Test Provider
VIBE_CODER_DEFAULT_PROVIDER=test-openai
VIBE_CODER_PROVIDER_NAME=test-openai

# Test Settings
VIBE_CODER_TEST_MODE=true
VIBE_CODER_MOCK_RESPONSES=true
VIBE_CODER_DISABLE_TELEMETRY=true
VIBE_CODER_DISABLE_UPDATES=true

# Cache Settings for Testing
VIBE_CODER_CACHE_TTL=60
VIBE_CODER_ENABLE_CACHE=true

# Rate Limiting for Testing
VIBE_CODER_RATE_LIMIT_REQUESTS=100
VIBE_CODER_RATE_LIMIT_WINDOW=60

# Analytics and Tracking (Disabled for Testing)
VIBE_CODER_TRACK_USAGE=false
VIBE_CODER_COLLECT_METRICS=false
VIBE_CODER_COST_TRACKING=false

# Security Settings for Testing
VIBE_CODER_VALIDATE_SSL=false
VIBE_CODER_TIMEOUT=30

# File System Settings for Testing
VIBE_CODER_TEMP_DIR=/tmp/vibe_coder_test
VIBE_CODER_BACKUP_DIR=/tmp/vibe_coder_test/backups
VIBE_CODER_RESULTS_DIR=/tmp/vibe_coder_test/results

# Git Settings for Testing
VIBE_CODER_GIT_AUTO_COMMIT=false
VIBE_CODER_GIT_BRANCH=test-branch
VIBE_CODER_GIT_REMOTE=origin

# Plugin Settings for Testing
VIBE_CODER_PLUGINS_ENABLED=false
VIBE_CODER_PLUGIN_DIR=/tmp/vibe_coder_test/plugins

# Intelligence Module Settings for Testing
VIBE_CODER_INTELLIGENCE_ENABLED=true
VIBE_CODER_AST_CACHE_TTL=300
VIBE_CODER_MAX_FILE_SIZE=10000

# Healing Module Settings for Testing
VIBE_CODER_AUTO_HEAL_ENABLED=true
VIBE_CODER_HEAL_RETRIES=3
VIBE_CODER_VALIDATE_SYNTAX=true

# MCP (Model Context Protocol) Settings for Testing
VIBE_CODER_MCP_ENABLED=false
VIBE_CODER_MCP_SERVERS=test-server1,test-server2

# Performance Settings for Testing
VIBE_CODER_CONCURRENT_REQUESTS=5
VIBE_CODER_BATCH_SIZE=10
VIBE_CODER_MEMORY_LIMIT=512

# Integration Test Settings
VIBE_CODER_INTEGRATION_TEST=true
VIBE_CODER_TEST_TIMEOUT=60
VIBE_CODER_TEST_DATA_DIR=./tests/data

# Logging Configuration for Testing
VIBE_CODER_LOG_FORMAT=json
VIBE_CODER_LOG_FILE=/tmp/vibe_coder_test/test.log
VIBE_CODER_LOG_LEVEL_TEST=DEBUG

# Database Settings (if using SQLite for testing)
VIBE_CODER_DB_PATH=/tmp/vibe_coder_test/test.db
VIBE_CODER_DB_TIMEOUT=10